{
  "name": "cy-parallel",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.1",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "node build.js",
    "bp": "yarn build && npm publish",
    "cy:report": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json && npx mochawesome-report-generator cypress/reports/merged-report.json -o cypress/reports/merged-html-report"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "cypress": "^13.15.2",
    "esbuild": "^0.24.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.13.0"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "keywords": [
    "cypress",
    "parallel",
    "parallelization",
    "cypress-parallel",
    "test-automation",
    "end-to-end",
    "e2e-testing",
    "automated-testing",
    "test-runner",
    "continuous-integration",
    "ci",
    "devops",
    "test-splitting",
    "test-optimization",
    "scalable-testing",
    "automation-tool"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "typescript": "^5.6.3"
  }
}
