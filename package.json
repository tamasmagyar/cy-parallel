{
  "name": "cy-parallel",
  "version": "0.2.0",
  "packageManager": "yarn@4.5.1",
  "main": "dist/index.js",
  "bin": "dist/run-parallel.js",
  "files": [
    "dist/run-parallel.js"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "build": "tsc",
    "publish:npm": "yarn build && yarn publish",
    "cy:report": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json && npx mochawesome-report-generator cypress/reports/merged-report.json -o cypress/reports/merged-html-report"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/node": "^22.9.0",
    "cypress": "^13.15.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "http://localhost:4873"
  }
}
